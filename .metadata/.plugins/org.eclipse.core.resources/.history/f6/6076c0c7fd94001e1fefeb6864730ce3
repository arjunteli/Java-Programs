package com.kn.BinaryTreeDemo;

public class Demo {

	public static void main(String[] args) {
		int num = 100;
		fibonacci(num);
	}
	
	public static void fibonacci(int num) {
		int a = 0;
		int b = 1;
		for(int i = 0; b<=num; i++) {
			System.out.print(a+" ");
			int temp = b;
			b = a+b;
			a = temp;
		}
	}

}
